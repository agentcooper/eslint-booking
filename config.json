{
    "env": {
        "browser": true
    },

    "globals": {
        "booking": true,
        "sNSStartup": true,
        "sNSExperiments": true,
        "sNSExperimentsLoad": true,
        "sNSStartupLoad": true,
        "jQuery": true,
        "$": true,
        "console": true
    },

    "rules": {
        "strict": 1,
        "no-console": 0,
        "quotes": [1, "single"],
        "camelcase": 0,
        "no-underscore-dangle": 0,
        "no-trailing-spaces": 1,
        "max-params": [1, 3],
        "max-statements": [1, 30],
        "max-len": [1, 90, 4],
        "max-nested-callbacks": [2, 3],
        "brace-style": "1tbs",
        "space-after-keywords": 1,
        "valid-jsdoc": [2, {
          "prefer": {
            "return": "returns",
            "requireReturn": false
          }
        }]
    }
}
